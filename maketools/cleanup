if ls *.for 1> /dev/null 2>&1; then
   rm *.for
fi
if ls *.err 1> /dev/null 2>&1; then
   rm *.err
fi
if ls *.o 1> /dev/null 2>&1; then
   rm *.o
fi
if ls *.a 1> /dev/null 2>&1; then
   rm *.a
fi
if ls *.run 1> /dev/null 2>&1; then
   rm *.run
fi
if ls orders.spc 1> /dev/null 2>&1; then
   rm orders.spc
fi
if ls orders.inc 1> /dev/null 2>&1; then
   rm orders.inc
fi
if ls orders.rin 1> /dev/null 2>&1; then
   rm orders.rin
fi


